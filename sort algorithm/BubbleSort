void BubbleSort (int a[],int len)
{
    for(int i=0;i<len-1;i++)
    {
        for(int j=len-1;j>i;j--)
        {
            if(a[j-1]>a[j])
            {
                swap(a[j-1],a[j]);
            }
        }
    }
    for(int k=0;k<len;k++)
    {
        cout<<a[k]<<" ";
    }
    cout<<endl;
}

void InsertionSort (int a[],int len)
{
    for(int i=1;i<len;i++)
    {
        int t=a[i];
        int j=i-1;
        while(j>=0&&t<a[j])
        {
            a[j+1]=a[j];
            j--;
        }
        a[j+1]=t;
    }
    for(int k=0;k<len;k++)
    {
        cout<<a[k]<<" ";
    }
    cout<<endl;
}
